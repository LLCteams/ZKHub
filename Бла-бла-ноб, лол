ff:Button("Gravity", function()
if _G.WRDGravitySwitchStarted == nil then
	_G.WRDGravitySwitchStarted = true
	
	--Gets the player's mouse
	local mouse = game:GetService("Players").LocalPlayer:GetMouse()

	--Waits until the player's mouse is found
	repeat wait() until mouse

	--Toggles
	mouse.KeyDown:connect(function(key)
		if key:lower() == "e" then
			if workspace.Gravity > 0 then
				--Turns off gravity
				workspace.Gravity = 0
				game.StarterGui:SetCore("SendNotification", {Title="Zero Gravity"; Text="Volt"; Duration=1;})
			else 
				--Reset to default
				workspace.Gravity = 196.2
				game.StarterGui:SetCore("SendNotification", {Title="Normal Gravity"; Text="Volt"; Duration=1;})
			end
		end
	end)
end

game.StarterGui:SetCore("SendNotification", {Title="Volt"; Text="The Gravity Switch is ready!\nPress \"e\" to toggle"; Duration=5;})
end)
ff:Button("Explode", function()
Player = game.Players.LocalPlayer
Character = Player.Character
 
function explosion()
	local sound = Instance.new("Sound")
	sound.Name = "explode"
	sound.SoundId = "rbxassetid://153274423"
	sound.Pitch = math.random(90, 110)*.01
	sound.Volume = math.random(2,5)*.15
	sound.Parent = script
	sound:Play()
	if Character.Humanoid.Health > 0 then
		for i = 0,5,1 do -- 5Î≤à Î∞òÎ≥µÏã§Ìñâ
			Character.Humanoid.CameraOffset = Vector3.new(math.random(-2,2)/10,math.random(-2,2)/10,math.random(-2,2)/10)
			wait(.05)
			Character.Humanoid.CameraOffset = Vector3.new(0,0,0)
			wait(.05)
		end
	end
end
explosion()
end)
