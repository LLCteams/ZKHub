local GUI = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/aaaa"))()

local UI = GUI:CreateWindow("Loader Super X","Welcome")

local Home = UI:addPage("Hub",1,true,6)

Home:addLabel("Loader","Fixed version")

Home:addButton("Loader [v2.3]",function()

    game.StarterGui:SetCore("SendNotification",{

        Title = "Loader by LLC";

        Text = "Welcome, Key is 208.33.22";

        loadstring(game:HttpGet("https://hastepaste.com/raw/IuVEUHl"))()

    })

end)

local LP = UI:addPage("Hub",2,false,6)

-- Label

LP:addLabel("Loader","Our Lastest version v2.5")

--- Button

LP:addButton("Loader [v2.5]",function()

    game.StarterGui:SetCore("SendNotification",{

        Title = "Loader by LLC";

        Text = "Get key from LLC Discord!";

        loadstring(game:HttpGet("https://raw.githubusercontent.com/LLCteams/ZKHub/main/77%D0%91%D0%BB%D0%BE%D0%BA%20%D0%BC%D0%B0%D0%BD%20%D1%81%D0%BA%D1%80%D0%B8%D0%BF%D1%82%20%D0%B4%D0%BB%D1%8F%20gg"))()

    })

end)

local LP = UI:addPage("Local Player",3,false,6)

-- Label

LP:addLabel("Beta","Players forneeded")

--- Button

LP:addButton("Infinity Yield",function()

    game.StarterGui:SetCore("SendNotification",{

        Title = "Loader by LLC";

        Text = "Infinity Yield";

        loadstring(game:HttpGet(('https://raw.githubusercontent.com/EdgeIY/infiniteyield/master/source'),true))()
        
    })

end)


LP:addButton("ESP",function()

    game.StarterGui:SetCore("SendNotification",{

        Title = "Loader by LLC";

        Text = "ESP";

        loadstring(game:HttpGet("https://raw.githubusercontent.com/LLCteams/ZKHub/main/%D0%A1%D1%83%D0%BF%D0%B5%D1%80%20%D1%83%20%D0%BC%D0%B5%D0%BD%D1%8F%20%D0%BD%D0%B5%D1%82"))()
        
    })

end)




LP:addToggle("HackSpeed",function(value)

    if value == false then

        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 16

    else

        game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = 26

    end

end)

LP:addToggle("HackJump",function(value)
    if value == false then
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 50
    else
        game.Players.LocalPlayer.Character.Humanoid.JumpPower = 140
    end
end)

LP:addToggle("HackGravity",function(value)

    if value == false then

        game.Workspace.Gravity = 100

    else

        game.Workspace.Gravity = 20

    end

end)

LP:addSlider("WalkSpeed",16,150,function(value)

    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value

end)

-- Textbox

LP:addTextBox("Jump Power / 50 is default","Number here",function(value)

    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value

end)
