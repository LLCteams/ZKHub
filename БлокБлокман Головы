        LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(0, 300, 0)
    end
    if LocalPlayer.Character.HumanoidRootPart.Anchored == false then
        LocalPlayer.Character.HumanoidRootPart.Anchored = true
    end

end
